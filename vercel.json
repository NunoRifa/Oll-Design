{
    "version": 2,
    "builds": [
        {
            "src": "resources/js/app.jsx",
            "use": "@vercel/static"
        },
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
