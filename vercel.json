{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        },
        {
            "src": "resources/js/app.jsx",
            "use": "@vercel/static-build",
            "config": { "distDir": "public/build" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/build/$1"
        }
    ]
}
